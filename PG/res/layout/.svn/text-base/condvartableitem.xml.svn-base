<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="?android:attr/listPreferredItemHeight">
	<LinearLayout android:id="@+id/cvti_dots"
		android:layout_alignParentRight="true"
		android:layout_gravity="center_vertical"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:orientation="horizontal">
	<ImageView android:id="@+id/cvti_comp1"
		android:layout_width="12dip"
		android:layout_height="12dip"
		android:gravity="center_vertical"
		android:src="@drawable/company1" />
	<ImageView android:id="@+id/cvti_comp2"
		android:layout_width="12dip"
		android:layout_height="12dip"
		android:layout_gravity="center_vertical"
		android:src="@drawable/company2" />
	<ImageView android:id="@+id/cvti_comp3"
		android:layout_gravity="center_vertical"
		android:layout_width="12dip"
		android:layout_height="12dip"
		android:src="@drawable/company3" />
		
	</LinearLayout>


<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_toLeftOf="@id/cvti_dots"
	android:layout_alignParentLeft="true"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:orientation="horizontal"
	android:paddingTop="4dip"
	android:paddingBottom="4dip">
	
	<TextView android:id="@+id/cvti_locus"
		android:layout_width="90dip"
		android:layout_height="wrap_content"
		android:textSize="16dip"
		android:textColor="#ff000000"
		android:text="locus"
		android:paddingLeft="2dip"
		/>
	<TextView android:id="@+id/cvti_gene"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:textSize="16dip"
		android:textColor="#ff000000"
		android:text="gene"
		android:lines="1"
		android:scrollHorizontally="true"
		android:layout_weight="1"
		/>
		
	<TextView android:id="@+id/cvti_rsid"
		android:layout_width="90dip"
		android:layout_height="wrap_content"
		android:textSize="16dip"
		android:textColor="#ff000000"
		android:text="RSID"
		/>		
		
	</LinearLayout>
	
	</RelativeLayout>
		
